{
    "process": "module_update_sa",
    "systemability": [
        {
            "name": 4103,
            "libpath": "libmodule_update_service.z.so",
            "run-on-create": false,
            "distributed": false,
            "dump_level": 1,
            "start-on-demand": {
                "param":[
                    {
                        "name":"persist.samgr.moduleupdate.start",
                        "value":"true"
                    },
                    {
                        "name":"persist.moduleupdate.bms.scan",
                        "value":"revert"
                    }
                ]
            },
            "stop-on-demand": {
                "param":[
                    {
                        "name":"bootevent.boot.completed",
                        "value":"true"
                    }
                ]
            }
        }
    ]
}